@model IEnumerable<appRegistroCivil.Models.Pais>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nbrPais)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.area)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.poblacionActual)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Audios.descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Imagenes.descripcion)
        </th>
        <th>
            Id Presidente Actual
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nbrPais)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.area)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.poblacionActual)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Audios.descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Imagenes.descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Persona2.nbrPersona)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idPais }) |
        </td>
    </tr>
}

</table>
